pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

function _init()
	cls()
	switch_case = 0
	train = {{64,8},{72,8},{80,8}}
end

function _update()
	if (btnp(4)) then
		adv_switch()
	end
	move_train()
end

function _draw()
	map(0,0,0,0,16,16)
	draw_switch()
	draw_train()
end

function draw_train()
	local len = count(train)
	for t = 1,len do
		if (t==1) then
			sprite = 11
		elseif (t==len) then
			sprite = 13
		else
			sprite = 12
		end
		spr(sprite,train[t][1],train[t][2])
	end
end

function draw_switch()
	if (switch_state==1) then
		sspr(0,16,16,16,56,56,16,16)
	else
		sspr(64,16,16,16,56,56,16,16)
	end
end

function move_segment(s, dir)
	spd = dir*2
	if (s[2]==8) then --top side
		if (s[1]==112) then --top right
			s[2]+=spd
		else
			s[1]+=spd
		end
	elseif (s[1]==112) then --right side
		if(s[2]==112) then --bottom right
			s[1]-=spd
		else
			s[2]+=spd
		end
	elseif (s[2]==112) then --bottom side
		if (s[1]==8) then --bottom side 
			s[2]-=spd
		else
			s[1]-=spd
		end
	elseif (s[1]==8) then --left side
		if (s[2]==8) then --top left
			s[1]+=spd
		else
			s[2]-=spd
		end
	end
end

function adv_switch()
	if (switch_state<1) then
		switch_state += 1
	else
		switch_state = 0
	end
end

function move_train()
	for t in all(train) do
		move_segment(t, switch_state)
	end
end

